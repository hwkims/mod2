(function($) {

	$.jBanner = {

		// Defaults
		defaults: {
			bannerList : '#bannerList',
			bannerSize : 170,
			pagingBtns : '#mainBanner .paging a',
			slideSpeed : 1000,
			slideTimer : null,
			currentPos : 0,
			currentIdx : 0
		},

		// Rotation + Timing Event
		autoSlide : function(config) {
			var btnsLength = $(config.pagingBtns).length;
			config.currentPos = parseInt($(config.bannerList).css('margin-top'));
			config.currentIdx = (-config.currentPos) / config.bannerSize + 1;

			if (config.currentIdx >= btnsLength) {
				config.currentIdx = 0;
			}

			$.jBanner.setAnimate(config, config.bannerSize * (-config.currentIdx));

			config.slideTimer = setTimeout(function() { $.jBanner.autoSlide(config); }, config.slideSpeed);
		},

		// Stop the rotation
		setHover : function(bannerArea, config) {
			bannerArea.hover(function() {
			    clearTimeout(config.slideTimer);
			},
			function() {
				config.slideTimer = setTimeout(function() { $.jBanner.autoSlide(config); }, config.slideSpeed);
			});
		},

		// Button On/Off
		setBtnHover : function(config) {
			var btnImgs = $(config.pagingBtns).find('img');
			btnImgs.hover(function(ev) {
				var img = $(ev.currentTarget);
				img.attr('src', function() { return img.attr('src').replace('_off', '_on'); });
			},
			function(ev) {
				var img = $(ev.currentTarget);
				imgIdx = btnImgs.index(img);
				if (config.currentIdx != imgIdx) {
					img.attr('src', function() { return img.attr('src').replace('_on', '_off'); });
				}
			});
		},

		// Activate the clicked paging
		setBtns : function(config) {
			$(config.pagingBtns).each(function(idx) {
				$(this).bind('click', function() {
					config.currentIdx = idx;
					$.jBanner.setAnimate(config, config.bannerSize * (-idx));
					return false;
				});
			});
		},

		// Animation + Button On/Off
		setAnimate : function(config, val) {
			$(config.bannerList).animate({
				'margin-top' : val
			}, 'fast');
			
			$(config.pagingBtns).each(function(i) {
				var img = $(this).find('img');
				if (i == config.currentIdx) {
					img.attr('src', function() { return img.attr('src').replace('_off', '_on'); });
				} else {
					img.attr('src', function() { return img.attr('src').replace('_on', '_off'); });
				}
			});
		}
	};

$.fn.extend({
	jBanner : function(options) {
		var config = $.extend({}, $.jBanner.defaults, options);

		config.slideTimer = setTimeout(function() { $.jBanner.autoSlide(config); }, config.slideSpeed);
		var img = $(config.pagingBtns).find('img').eq(0);
		img.attr('src', function() { return img.attr('src').replace('off', 'on'); });

		$.jBanner.setHover(this, config);
		$.jBanner.setBtns(config);
		$.jBanner.setBtnHover(config);
	}
});

})(jQuery);

